@Article{Brooks2011,
  Title                    = {Adaptation of cells to new environments.},
  Author                   = {Brooks, Aaron N. and Turkarslan, Serdar and Beer, Karlyn D. and Lo, Fang Yin and Baliga, Nitin S.},
