  Timestamp                = {2014.05.19}
}

@Article{Li2004,
  Title                    = {The yeast cell-cycle network is robustly designed},
  Author                   = {Li, F. T. and Long, T. and Lu, Y. and Ouyang, Q. and Tang, C.},
  Journal                  = {Proceedings of the National Academy of Sciences of the United States of America},
  Year                     = {2004},

  Month                    = apr,
  Note                     = {{WOS:000220761200013}},
  Number                   = {14},
  Pages                    = {4781--4786},
  Volume                   = {101},
  Abstract                 = {The interactions between proteins, {DNA}, and {RNA} in living cells constitute molecular networks that govern various cellular functions. To investigate the global dynamical properties and stabilities of such networks, we studied the cell-cycle regulatory network of the budding yeast. With the use of a simple dynamical model, it was demonstrated that the cell-cycle network is extremely stable and robust for its function. The biological stationary state, the G, state, is a global attractor of the dynamics. The biological pathway, the cell-cycle sequence of protein states, is a globally attracting trajectory of the dynamics. These properties are largely preserved with respect to small perturbations to the network. These results suggest that cellular regulatory networks are robustly designed for their functions.},
  Doi                      = {10.1073/pnas.0305937101},
  ISSN                     = {0027-8424},
  Owner                    = {dreiss},
  Timestamp                = {2014.05.19}
}

