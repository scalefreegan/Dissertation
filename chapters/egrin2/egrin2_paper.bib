@Article{Cho2011a,
  Title                    = {The PurR regulon in Escherichia coli K-12 MG1655.},
  Author                   = {Cho, Byung-Kwan and Federowicz, Stephen A. and Embree, Mallory and Park, Young-Seoub and Kim, Donghyuk and Palsson, Bernhard Ø.},
