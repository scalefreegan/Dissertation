@Article{Nelson1999,
  Title                    = {Evidence for lateral gene transfer between Archaea and Bacteria from genome sequence of Thermotoga maritima},
  Author                   = {Nelson, K. E. and Clayton, R. A. and Gill, S. R. and Gwinn, M. L. and Dodson, R. J. and Haft, D. H. and Hickey, E. K. and Peterson, L. D. and Nelson, W. C. and Ketchum, K. A. and {McDonald}, L. and Utterback, T. R. and Malek, J. A. and Linher, K. D. and Garrett, M. M. and Stewart, A. M. and Cotton, M. D. and Pratt, M. S. and Phillips, C. A. and Richardson, D. and Heidelberg, J. and Sutton, G. G. and Fleischmann, R. D. and Eisen, J. A. and White, O. and Salzberg, S. L. and Smith, H. O. and Venter, J. C. and Fraser, C. M.},
