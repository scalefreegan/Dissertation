  Author                   = {Lee, T. I. and Rinaldi, N. J. and Robert, F. and Odom, D. T. and Bar-Joseph, Z. and Gerber, G. K. and Hannett, N. M. and Harbison, C. T. and Thompson, C. M. and Simon, I. and Zeitlinger, J. and Jennings, E. G. and Murray, H. L. and Gordon, D. B. and Ren, B. and Wyrick, J. J. and Tagne, J. B. and Volkert, T. L. and Fraenkel, E. and Gifford, D. K. and Young, R. A.},
  Journal                  = {Science},
