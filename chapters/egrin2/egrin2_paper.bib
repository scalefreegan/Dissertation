  Timestamp                = {2014.05.19}
}

@Article{Nichols2011,
  Title                    = {Phenotypic landscape of a bacterial cell},
  Author                   = {Nichols, Robert J and Sen, Saunak and Choo, Yoe Jin and Beltrao, Pedro and Zietek, Matylda and Chaba, Rachna and Lee, Sueyoung and Kazmierczak, Krystyna M and Lee, Karis J and Wong, Angela and Shales, Michael and Lovett, Susan and Winkler, Malcolm E and Krogan, Nevan J and Typas, Athanasios and Gross, Carol A},
  Journal                  = {Cell},
  Year                     = {2011},

  Month                    = jan,
  Note                     = {{PMID:} 21185072 {PMCID:} {PMC3060659}},
  Number                   = {1},
  Pages                    = {143--156},
  Volume                   = {144},
  Abstract                 = {The explosion of sequence information in bacteria makes developing high-throughput, cost-effective approaches to matching genes with phenotypes imperative. Using E. coli as proof of principle, we show that combining large-scale chemical genomics with quantitative fitness measurements provides a high-quality data set rich in discovery. Probing growth profiles of a mutant library in hundreds of conditions in parallel yielded {\textgreater} 10,000 phenotypes that allowed us to study gene essentiality, discover leads for gene function and drug action, and understand higher-order organization of the bacterial chromosome. We highlight new information derived from the study, including insights into a gene involved in multiple antibiotic resistance and the synergy between a broadly used combinatory antibiotic therapy, trimethoprim and sulfonamides. This data set, publicly available at http://ecoliwiki.net/tools/chemgen/, is a valuable resource for both the microbiological and bioinformatic communities, as it provides high-confidence associations between hundreds of annotated and uncharacterized genes as well as inferences about the mode of action of several poorly understood drugs.},
  Doi                      = {10.1016/j.cell.2010.11.052},
  ISSN                     = {1097-4172},
  Keywords                 = {Escherichia coli, Gene Deletion, Gene Expression Profiling, Genome, Bacterial, Genomics, Mutation},
  Language                 = {eng},
  Owner                    = {dreiss},
  Timestamp                = {2014.05.19}
}

