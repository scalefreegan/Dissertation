@Article{Makarova2006,
  Title                    = {The {HicAB} cassette, a putative novel, {RNA-targeting} toxin-antitoxin system in archaea and bacteria},
  Author                   = {Makarova, Kira S. and Grishin, Nick V. and Koonin, Eugene V.},
